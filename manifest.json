{
  "manifest_version": 2,
  "name": "Multilingual Fox",
  "version": "0.2",
  "description": "A simple dictionary extension for firefox",
  "icons": {
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "*://localhost/*",
    "*://endic.naver.com/*",
    "*://zh.dict.naver.com/*",
    "*://ja.dict.naver.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/index.js"]
    }
  ]
}